<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--SwitchButton-->
    <declare-styleable name="SwitchButton">
        <attr name="backPadding" format="dimension" />
        <attr name="defaultRoundBtnColor" format="color" />
        <attr name="openedRoundBtnColor" format="color" />
        <attr name="backColorOn" format="color" />
        <attr name="backColorOff" format="color" />
    </declare-styleable>
    <!--RoundImageView-->
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>
</resources>